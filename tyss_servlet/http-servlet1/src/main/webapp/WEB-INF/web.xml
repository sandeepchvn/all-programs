<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>MyRegister</servlet-name>
		<servlet-class>read_form_data.Register</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyRegister</servlet-name>
		<url-pattern>/user-register</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MydoPost</servlet-name>
		<servlet-class>read_form_data.PostMethod</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MydoPost</servlet-name>
		<url-pattern>/do-post</url-pattern>
	</servlet-mapping>
	<servlet>
	<servlet-name>ProductServlet</servlet-name>
	<servlet-class>read_form_data.Product</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ProductServlet</servlet-name>
		<url-pattern>/product-info</url-pattern>
	</servlet-mapping>
</web-app>
