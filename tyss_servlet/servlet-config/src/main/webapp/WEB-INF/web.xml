<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>CalculaterServlet</servlet-name>
		<servlet-class>servlet_config.Calculater</servlet-class>
		<init-param>
			<param-name>pi</param-name>
			<param-value>3.14</param-value>
		</init-param>
		<init-param>
			<param-name>rate</param-name>
			<param-value>6.91</param-value>
		</init-param>
	</servlet>
	<servlet-mapping>
		<servlet-name>CalculaterServlet</servlet-name>
		<url-pattern>/calcu</url-pattern>
	</servlet-mapping>
</web-app>
