<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>MyUSer</servlet-name>
		<servlet-class>read_form_data.UserInfo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyUSer</servlet-name>
