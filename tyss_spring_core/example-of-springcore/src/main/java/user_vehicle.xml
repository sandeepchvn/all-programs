<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="myUser" class="user_vehicles.User">
		<property name="name" value="sandeep">
		</property>
		<property name="phone" value="7676806084">
		</property>
		<property name="vehicles">
			<list>
				<ref bean="myVehicle1" />
				<ref bean="myVehicle2" />
			</list>

		</property>
	</bean>

	<bean id="myVehicle1" class="user_vehicles.Vehicle">
		<property name="num" value="56"></property>
		<property name="name" value="tjh"></property>
		<property name="price" value="678"></property>
	</bean>
	<bean id="myVehicle2" class="user_vehicles.Vehicle">
		<property name="num" value="56"></property>
		<property name="name" value="tjh2"></property>
		<property name="price" value="678"></property>
	</bean>

</beans>