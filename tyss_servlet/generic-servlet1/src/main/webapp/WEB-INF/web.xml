	</servlet-mapping>
	<servlet>
	<servlet-name>EmployeeInfo</servlet-name>
	<servlet-class>read_form_data.EmployeeInfo</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>EmployeeInfo</servlet-name>
	<url-pattern>/emp</url-pattern>
	</servlet-mapping>
</web-app>
